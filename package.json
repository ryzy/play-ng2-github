{
  "name": "play-ng-github",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port=4100",
    "start:prod": "ng serve --port=4100 --aot=true -prod",
    "build": "rimraf dist/ && ng build --verbose --aot=false",
    "build:prod": "rimraf dist/ && ng build --progress=false --sourcemap=false --aot=true --verbose -prod",
    "deploy": "firebase deploy --only=hosting --project default --token=$FIREBASE_TOKEN",
    "deploy:prod": "firebase deploy --only=hosting --project prod --token=$FIREBASE_TOKEN",
    "lint": "ng lint",
    "test": "ng test",
    "test:ci": "ng test --code-coverage --progress=false --single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/router": "4.0.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.2",
    "@ngrx/router-store": "1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.4",
    "@webcomponents/custom-elements": "1.0.0-rc.3",
    "bootstrap": "4.0.0-alpha.6",
    "clarity-angular": "0.8.14",
    "clarity-icons": "0.8.14",
    "clarity-ui": "0.8.14",
    "core-js": "2.4.1",
    "ngrx-store-freeze": "0.1.9",
    "reselect": "3.0.0",
    "rxjs": "5.3.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.1",
    "@types/jasmine": "2.5.45",
    "@types/node": "6.0.60",
    "codelyzer": "2.1.1",
    "firebase-tools": "3.6.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.1",
    "rimraf": "^2.6.1",
    "ts-node": "3.0.2",
    "tslint": "4.5.0",
    "typescript": "2.2.2"
  }
}
