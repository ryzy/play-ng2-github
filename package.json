{
  "name": "play-ng-github",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port=4100",
    "start:prod": "ng serve --port=4100 --aot=true -prod",
    "build": "ng build",
    "build:prod": "ng build --progress=false --sourcemap=false --aot=true -prod",
    "deploy": "firebase deploy --only=hosting --project default --token=$FIREBASE_TOKEN",
    "deploy:prod": "firebase deploy --only=hosting --project prod --token=$FIREBASE_TOKEN",
    "lint": "ng lint --type-check",
    "test": "ng test --sourcemaps=false",
    "test:ci": "ng test --code-coverage --progress=false --single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e --webdriver-update=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.4.5",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/common": "4.4.5",
    "@angular/compiler": "4.4.5",
    "@angular/core": "4.4.5",
    "@angular/forms": "4.4.5",
    "@angular/http": "4.4.5",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "4.4.5",
    "@angular/platform-browser-dynamic": "4.4.5",
    "@angular/router": "4.4.5",
    "@ngrx/effects": "4.0.5",
    "@ngrx/router-store": "4.0.4",
    "@ngrx/store": "4.0.3",
    "@webcomponents/custom-elements": "1.0.4",
    "bootstrap": "4.0.0-alpha.6",
    "clarity-angular": "0.10.9",
    "clarity-icons": "0.10.9",
    "clarity-ui": "0.10.9",
    "core-js": "2.5.1",
    "rxjs": "5.4.3",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.4.7",
    "@angular/compiler-cli": "4.4.5",
    "@angular/language-service": "4.4.5",
    "@ngrx/store-devtools": "4.0.0",
    "@types/jasmine": "2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "8.0.37",
    "codelyzer": "3.2.1",
    "firebase-tools": "3.13.1",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.2",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.5.3"
  }
}
