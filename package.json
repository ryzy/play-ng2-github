{
  "name": "play-ng-github",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port=4100",
    "start:prod": "ng serve --port=4100 --aot -prod",
    "build": "rimraf dist/ && ng build --verbose",
    "build:prod": "rimraf dist/ && ng build -prod --progress=false --verbose --aot",
    "deploy": "firebase deploy --only=hosting --project default --token=$FIREBASE_TOKEN",
    "deploy:prod": "firebase deploy --only=hosting --project prod --token=$FIREBASE_TOKEN",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "test:ci": "ng test --code-coverate --progress=false --single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/material": "latest",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.2",
    "core-js": "2.4.1",
    "ngrx-store-freeze": "^0.1.5",
    "reselect": "2.5.4",
    "rxjs": "5.0.1",
    "ts-helpers": "1.1.2",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.24",
    "codelyzer": "~2.0.0-beta.1",
    "firebase-tools": "^3.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "ts-node": "1.2.1",
    "tslint": "4.0.2",
    "typescript": "2.0.10",
    "webdriver-manager": "10.2.5"
  }
}
